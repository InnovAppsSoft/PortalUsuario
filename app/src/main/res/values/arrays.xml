<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CodeScannerView">
        <attr format="color" name="maskColor"/>
        <attr format="color" name="frameColor"/>
        <attr format="color" name="autoFocusButtonColor"/>
        <attr format="color" name="flashButtonColor"/>
        <attr format="boolean" name="autoFocusButtonVisible"/>
        <attr format="boolean" name="flashButtonVisible"/>
        <attr format="dimension" name="frameThickness"/>
        <attr format="dimension" name="frameCornersSize"/>
        <attr format="dimension" name="frameCornersRadius"/>
    </declare-styleable>
    <!-- Reply Preference -->
    <string-array name="temas_ui">
        <item>Sistema</item>
        <item>Claro</item>
        <item>Oscuro</item>
    </string-array>

    <string-array name="temas_ui_values">
        <item>sistema</item>
        <item>claro</item>
        <item>oscuro</item>
    </string-array>

</resources>